{
  "watch": [
    "server.js",
    "routes/",
    "models/",
    "middleware/",
    "scripts/"
  ],
  "ext": "js,json",
  "ignore": [
    "node_modules/",
    "logs/",
    "*.log"
  ],
  "delay": 1000,
  "env": {
    "NODE_ENV": "development"
  },
  "restartable": "rs",
  "verbose": true,
  "legacyWatch": true
}
